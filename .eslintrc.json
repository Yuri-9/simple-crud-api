{
  "env": {
    "commonjs": true,
    "node": true,
    "jest/globals": true
  },
  "extends": ["eslint:recommended", "airbnb-base", "prettier"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "no-plusplus": 0,
    "quotes": [1, "single", { "avoidEscape": true }],
    "no-unused-vars": "warn",
    "arrow-body-style": 0,
    "no-underscore-dangle": 0
  }
}
